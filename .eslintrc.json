{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-param-reassign": ["error",
            {
              "props": true,
              "ignorePropertyModificationsFor": ["connection"]
            }
          ]
    },
    "globals": {
      "PayloadTypes": true,
      "ResponseTypes": true,
      "chatboxElements": true
  }
}
